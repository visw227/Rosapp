{
  "expo": {
    "name": "Rosnet",
    "slug": "rosnet-app",
    "privacy": "unlisted",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.rosnet.mobile", 
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "Rosnet would like to use Face ID for security purposes."
      }
    },
    "android": {
			"package": "com.rosnet.mobile"
		}
  }
}